{
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "bluebird": "^3.5.5",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "knex": "^0.19.1",
    "lodash.range": "^3.2.0",
    "pg": "^7.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "knex": "knex --knexfile src/server/knexfile.js",
    "test": "babel-node ./node_modules/.bin/mocha --recursive",
    "watch:scss": "node-sass --watch src/client/client.scss dist/client.css",
    "build:scss": "node-sass src/client/client.scss dist/client.css",
    "watch:server": "nodemon --exec babel-node -- bin/serve",
    "watch": "npm-run-all --parallel watch:scss watch:server",
    "build": "npm-run-all --parallel build:scss",
    "serve": "babel-node bin/serve",
    "postinstall": "yarn build"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "engines": {
    "node": "8.11.3"
  }
